{
    "cli": {
      "version": ">= 3.8.1",
      "promptToConfigurePushNotifications": false
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "ios": {
          "resourceClass": "m-medium"
        },
        "android": {
          "buildType": "apk"
        }
      },
      "preview": {
        "distribution": "internal",
        "ios": {
          "resourceClass": "m-medium",
          "simulator": true
        },
        "android": {
          "buildType": "apk"
        }
      },
      "production": {
        "ios": {
          "resourceClass": "m-medium"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
  